<?php

/**
 * @file
 * Contains gos_migrate.module.
 */

/**
 * Implements hook_migration_plugins_alter().
 */
function gos_migrate_migration_plugins_alter(array &$definitions) {
  $definitions['gos_platforms']['source']['path'] = drupal_get_path('module', 'gos_migrate') . $definitions['gos_platforms']['source']['path'];
}
