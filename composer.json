{
    "name": "drupal/gos-server",
    "description": "Games of Switzerland Drupal project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Toni Fisler",
            "role": "Frontend Developer"
        },
        {
            "name": "Kevin Wenger",
            "role": "Backend Developer"
        },
        {
            "name": "Camille LÃ©tang",
            "role": "UX Designer & Web Project Manager"
        },
        {
            "name": "Pierre Georges",
            "role": "Designer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/elasticsearch_helper",
                "type": "drupal-module",
                "version": "dev-6.x",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/elasticsearch_helper.git",
                    "reference": "1443af5e75545352d8e55c89fae2bd273a838a3e"
                },
                "require": {
                    "elasticsearch/elasticsearch": "~6.0"
                },
                "require-dev": {
                    "drupal/elasticsearch_helper_content": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "elasticsearch/elasticsearch",
                "type": "library",
                "version": "v6.8.2",
                "source": {
                    "type": "git",
                    "url": "https://github.com/elastic/elasticsearch-php.git",
                    "reference": "619c78266999c6e431df9ca0f844e8f656ac145b"
                },
                "require": {
                    "ext-json": ">=1.3.7",
                    "ezimuel/ringphp": "^1.1.2",
                    "php": "^7.3 || ^8.0 || ^8.1",
                    "psr/log": "~1.0 || ^3.0.0"
                },
                "require-dev": {
                    "doctrine/inflector": "^1.3",
                    "mockery/mockery": "^1.2",
                    "phpstan/phpstan": "^0.12",
                    "phpunit/phpunit": "^9.3",
                    "squizlabs/php_codesniffer": "^3.4",
                    "symfony/finder": "~4.0",
                    "symfony/yaml": "~4.0",
                    "symplify/git-wrapper": ">=9.0 <9.3.27"
                },
                "autoload": {
                    "files": [
                        "src/autoload.php"
                    ],
                    "psr-4": {
                        "Elasticsearch\\": "src/Elasticsearch/"
                    }
                }
            }
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "ext-exif": "*",
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/admin_toolbar": "^3.3",
        "drupal/bamboo_twig": "^6.0",
        "drupal/cdn": "^4.0",
        "drupal/ckeditor": "^1.0",
        "drupal/consumer_image_styles": "^4.0",
        "drupal/consumers": "^1.16",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/elasticsearch_helper": "dev-6.x",
        "drupal/field_group": "^3.0",
        "drupal/fieldable_path": "^1.0@RC",
        "drupal/focal_point": "^2.0",
        "drupal/gin": "^3.0",
        "drupal/gin_login": "^2.0",
        "drupal/hal": "^2.0",
        "drupal/image_effects": "^3.0",
        "drupal/jsonapi_extras": "^3.23",
        "drupal/metatag": "^1.14",
        "drupal/migrate_file": "^2.0",
        "drupal/migrate_source_csv": "^3.4",
        "drupal/migrate_tools": "^6.0",
        "drupal/new_relic_rpm": "^2.1",
        "drupal/pathauto": "^1.6",
        "drupal/restui": "^1.18",
        "drupal/simple_sitemap": "^4.1",
        "drupal/symfony_mailer": "^1.0@alpha",
        "drupal/views_ef_fieldset": "^1.4",
        "drupal/webp": "^1.0@beta",
        "drush/drush": "^12"
    },
    "require-dev": {
        "alexandresalome/mailcatcher": "^1.2",
        "behat/behat": "^3.5",
        "drupal/aggregator": "^2.2",
        "drupal/core-dev": "^10.0",
        "drupal/default_content": "^2.0",
        "drupal/drupal-extension": "^5.0",
        "drupal/rdf": "^2.1",
        "drupal/upgrade_status": "^4.0",
        "imbo/behat-api-extension": "^2.2",
        "marcortola/behat-seo-contexts": "^4.0",
        "mglaman/phpstan-drupal": "^1.1",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "composer/package-versions-deprecated": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Behat\\": "behat"
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/admin_toolbar": {
                "[3121816] - Version 8.x-2.1 need Drupal 8.8.0 or greater": "https://www.drupal.org/files/issues/2020-03-30/3121816-07.patch"
            },
            "drupal/core": {
                "[2429699] - Add Views EntityReference filter to be available for all entity reference fields": "https://www.drupal.org/files/issues/2022-04-07/drupal-views-entity-reference-filter-2429699-476.patch"
            },
            "drupal/metatag": {
                "[2945817] - Support JSON API, REST, GraphQL and custom normalizations": "https://www.drupal.org/files/issues/2023-11-30/metatag-n2945817-186.patch",
                "[3363582] - Resolve problems with field serialization changes": "https://www.drupal.org/files/issues/2023-05-30/metatag-n3363582-2.patch"
            },
            "drupal/restui": {
                "[3310517] - PHP Fatal error: Declaration of RestUITest::setUp() must be compatible with BrowserTestBase::setUp(): void": "https://git.drupalcode.org/project/restui/-/merge_requests/2.patch"
            },
            "drupal/elasticsearch_helper": {
                "[Local] - Drupal 10 readiness": "./patches/3113356-18.patch",
                "[Local] - Drupal 10 readiness Access Check": "./patches/elasticsearch_helper--d10-compliancy--accessCheck.patch"
            }
        },
        "autoload-drupal": {
            "modules": [
                "web/modules/contrib",
                "web/core/modules",
                "web/modules/custom"
            ]
        }
    }
}
